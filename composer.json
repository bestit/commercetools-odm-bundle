{
    "autoload": {
        "psr-4": {
            "BestIt\\CommercetoolsODMBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestIt\\CommercetoolsODMBundle\\Tests\\": "tests/"
        }
    },
    "name": "bestit/commercetools-odm-bundle",
    "description": "Makes the commercetools database accessible with the doctrine commons api.",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "type": "lib",
    "require": {
        "php": "^7.0",
        "bestit/commercetools-odm": "^1.0@dev",
        "symfony/dependency-injection": "^3.1 || ^4.0",
        "symfony/yaml": "^3.1 || ^4.0",
        "symfony/http-kernel": "^3.1 || ^4.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6",
        "phpunit/phpunit": "^5.5",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bjoern Lange",
            "email": "lange@bestit-online.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
