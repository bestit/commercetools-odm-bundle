        "psr-4": {
            "BestIt\\CommercetoolsODMBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestIt\\CommercetoolsODMBundle\\Tests\\": "tests/"
        }
    },
    "name": "bestit/commercetools-odm-bundle",
