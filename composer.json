    "require": {
        "php": "^7.0",
        "bestit/commercetools-odm": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6",
        "phpunit/phpunit": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bjoern Lange",
            "email": "lange@bestit-online.de"
        }
